<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<wsdl:definitions xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap12/" 
				  xmlns:u="http://www.universal-devices.com/wsdk/isy/3.0" 
				  xmlns:uo="http://www.universal-devices.com/wsdk/isy/3.0/objects" 
				  xmlns:ue="http://www.universal-devices.com/wsdk/isy/3.0/events" 
				  xmlns:uupb="http://www.universal-devices.com/wsdk/isy/3.0/objects/upb"
				  xmlns:uinsteon="http://www.universal-devices.com/wsdk/isy/3.0/objects/insteon"
				  xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" 
				  xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
				  name="udi" targetNamespace="http://www.universal-devices.com/wsdk/isy/3.0">
	<wsdl:documentation>
		Copyright 2007-2012 Universal Devices, Inc.
		All Rights Reserved

		Web Services Flag, Objects, Parameters, Messages, and Bindings for ISY
	</wsdl:documentation>

	<wsdl:types>
	<!-- following is used as a workaround for VS bug -->
	<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
				xmlns:u="http://www.universal-devices.com/wsdk/isy/3.0" 
				xmlns:uo="http://www.universal-devices.com/wsdk/isy/3.0/objects" 
				xmlns:ue="http://www.universal-devices.com/wsdk/isy/3.0/events" 
				xmlns:uupb="http://www.universal-devices.com/wsdk/isy/3.0/objects/upb"
				xmlns:uinsteon="http://www.universal-devices.com/wsdk/isy/3.0/objects/insteon"
				targetNamespace="http://www.universal-devices.com/wsdk/isy/3.0"
				elementFormDefault="unqualified">

	  <xsd:import namespace="http://www.universal-devices.com/wsdk/isy/3.0/objects" schemaLocation="./udiobjs.xsd"/>
	  <xsd:import namespace="http://www.universal-devices.com/wsdk/isy/3.0/events" schemaLocation="./udievnts.xsd"/>

	<!-- PARAMETERS -->
	<xsd:complexType name="IDParam">
		<xsd:sequence>
			<xsd:element name="id" minOccurs="1" maxOccurs="1" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="NameParam">
		<xsd:sequence>
			<xsd:element name="name" minOccurs="1" maxOccurs="1" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="IDNameParam">
		<xsd:complexContent>
			<xsd:extension base="u:IDParam">
				<xsd:sequence>
					<xsd:element name="name" minOccurs="1" maxOccurs="1" type="xsd:string"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="QueryParam">
		<xsd:sequence>
			<xsd:element name="node" minOccurs="1" maxOccurs="1" type="xsd:string"/>
			<xsd:element name="flag" minOccurs="1" maxOccurs="1" type="uo:NodeTypeFlag"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="CredentialsParam">
		<xsd:sequence>
			<xsd:element name="name" minOccurs="1" maxOccurs="1" type="xsd:string"/>
			<xsd:element name="password" minOccurs="1" maxOccurs="1" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="IDNameNodeTypeParam">
		<xsd:complexContent>
			<xsd:extension base="u:IDNameParam">
				<xsd:sequence>
					<xsd:element name="flag" minOccurs="1" maxOccurs="1" type="uo:NodeTypeFlag"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="AddNodeParam">
		<xsd:complexContent>
			<xsd:extension base="u:IDNameParam">
				<xsd:sequence>
					<xsd:element name="type" minOccurs="1" maxOccurs="1" type="xsd:string"/>
					<xsd:element name="flag" minOccurs="1" maxOccurs="1" type="uo:NodeOperationsFlag"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="NodeControllerParam">
		<xsd:sequence>
			<xsd:element name="node" minOccurs="1" maxOccurs="1" type="xsd:string"/>
			<xsd:element name="controller" minOccurs="1" maxOccurs="1" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="UDIServiceParam">
		<xsd:annotation>
			<xsd:documentation>
		 		In the case of X10:
		 		1. User "X10" as the control
		 		2. action may be null
		 		3. use house code and unit code in node. The format is:
		 		HouseCodeUnitCode (UnitCode may be null)
		 	</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="control" minOccurs="1" maxOccurs="1" type="uo:ControlType"/>
			<xsd:element name="action" minOccurs="0" maxOccurs="1" type="xsd:string"/>
			<xsd:element name="node" minOccurs="1" maxOccurs="1" type="xsd:string"/>
			<xsd:element name="flag" minOccurs="1" maxOccurs="1" type="uo:NodeTypeFlag"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="NodeGroupParam">
		<xsd:sequence>
			<xsd:element name="node" minOccurs="1" maxOccurs="1" type="xsd:string"/>
			<xsd:element name="group" minOccurs="1" maxOccurs="1" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="NodeParam">
		<xsd:sequence>
			<xsd:element name="node" minOccurs="1" maxOccurs="1" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>	

	<xsd:complexType name="MoveNodeParam">
		<xsd:complexContent>
			<xsd:extension base="u:NodeGroupParam">
				<xsd:sequence>
					<xsd:element name="flag" minOccurs="1" maxOccurs="1" type="uo:LinkModeFlag"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="DiscoverNodesParam">
		<xsd:annotation>
			<xsd:documentation>
				Optionally, provide the type of node to be discovered
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="type" minOccurs="0" maxOccurs="1" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="StopNodesDiscoveryParam">
		<xsd:sequence>
			<xsd:element name="flag" minOccurs="1" maxOccurs="1" type="uo:NodeOperationsFlag"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="InternetAccessParam">
		<xsd:sequence>
			<xsd:element name="flag" minOccurs="1" maxOccurs="1" type="uo:InternetAccessFlag"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="LinkingModeParam">
		<xsd:sequence>
			<xsd:element name="flag" minOccurs="1" maxOccurs="1" type="uo:LinkModeFlag"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="RestoreDevicesParam">
		<xsd:sequence>
			<xsd:element name="flag" minOccurs="1" maxOccurs="1" type="uo:RestoreDevicesFlag"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="RestoreDeviceParam">
		<xsd:sequence>
			<xsd:element name="node" minOccurs="1" maxOccurs="1" type="xsd:string"/>
			<xsd:element name="flag" minOccurs="1" maxOccurs="1" type="uo:RestoreDevicesFlag"/>
		</xsd:sequence>
	</xsd:complexType>	

	<xsd:complexType name="ReplaceDeviceParam">
		<xsd:sequence>
			<xsd:element name="node" minOccurs="1" maxOccurs="1" type="xsd:string"/>
			<xsd:element name="NewNode" minOccurs="1" maxOccurs="1" type="xsd:string"/>
			<xsd:element name="firmware" minOccurs="1" maxOccurs="1" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="SecuritySystemActionParam">
		<xsd:annotation>
			<xsd:documentation>
		 		Provide the command to be processed in the SecAction tag for example, see ELKActionCodes Provide the security code in the code tag
		 	</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="SecAction" minOccurs="1" maxOccurs="1" type="xsd:string"/>
			<xsd:element name="code" minOccurs="1" maxOccurs="1" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="SetDebugLevelParam">
		<xsd:sequence>
			<xsd:element name="option" minOccurs="1" maxOccurs="1" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="SetSceneProfileParam">
		<xsd:complexContent>
			<xsd:extension base="u:NodeControllerParam">
				<xsd:sequence>
					<xsd:element name="control" minOccurs="1" maxOccurs="1" type="uo:ControlType"/>
					<xsd:element name="action" minOccurs="1" maxOccurs="1" type="xsd:string"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="SubscriptionParam">
		<xsd:sequence>
			<xsd:element name="reportURL" minOccurs="1" maxOccurs="1" type="xsd:string"/>
			<xsd:element name="duration" minOccurs="1" maxOccurs="1" type="xsd:string"/>
			<xsd:element name="SID" minOccurs="0" maxOccurs="1" type="xsd:string"/>
			<!-- SID should be used in case of renewing subscriptions -->
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="UnsubscriptionParam">
		<xsd:sequence>
			<xsd:element name="SID" minOccurs="1" maxOccurs="1" type="xsd:string"/>
			<xsd:element name="flag" minOccurs="0" maxOccurs="1" type="xsd:int">
				<xsd:annotation>
					<xsd:documentation>
			 			flag is optional:
			 			0 (or if not supplied): Return error if not currently subscribed
			 			1 : Do not return an error if not currently subscribed
			 		</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="IsSubscribedParam">
		<xsd:sequence>
			<xsd:element name="SID" minOccurs="1" maxOccurs="1" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="CurrentSystemStatusParam">
		<xsd:sequence>
			<xsd:element name="SID" minOccurs="1" maxOccurs="1" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="SetNodeEnabledParam">
		<xsd:sequence>
			<xsd:element name="node" minOccurs="1" maxOccurs="1" type="xsd:string"/>
			<xsd:element name="flag" minOccurs="1" maxOccurs="1" type="uo:SetNodeEnabledFlag"/>
		</xsd:sequence>
	</xsd:complexType>	
	
	<xsd:complexType name="NodeHierarchyParam">
		<xsd:sequence>
			<xsd:element name="node" minOccurs="1" maxOccurs="1" type="xsd:string"/>
			<xsd:element name="nodeType" minOccurs="1" maxOccurs="1" type="uo:NodeHierarchyFlag"/>
			<xsd:element name="parent" minOccurs="0" maxOccurs="1" type="xsd:string"/>
			<xsd:element name="parentType" minOccurs="0" maxOccurs="1" type="uo:NodeHierarchyFlag"/>
		</xsd:sequence>
	</xsd:complexType>	
	
	<xsd:complexType name="DDNSHostParam">
		<xsd:sequence>
			<xsd:element name="host" minOccurs="1" maxOccurs="1" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>	
	
	<xsd:complexType name="DDNSHostAndIPParam">
		<xsd:sequence>
			<xsd:element name="host" minOccurs="1" maxOccurs="1" type="xsd:string"/>
			<xsd:element name="ip" minOccurs="1" maxOccurs="1" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>	
	
	<xsd:complexType name="BatchModeParam">
		<xsd:sequence>
			<xsd:element name="flag" minOccurs="1" maxOccurs="1" type="uo:BatchModeFlag"/>
		</xsd:sequence>
	</xsd:complexType>	

	<xsd:complexType name="BatteryDeviceWriteModeParam">
		<xsd:sequence>
			<xsd:element name="flag" minOccurs="1" maxOccurs="1" type="uo:BatteryDeviceWriteFlag"/>
		</xsd:sequence>
	</xsd:complexType>	
	
	<xsd:complexType name="PowerInfoParam">
			<xsd:annotation>
					<xsd:documentation>
						Only available on Orchestrator series
					</xsd:documentation>
				</xsd:annotation>
			<xsd:sequence>
			<xsd:element name="node" minOccurs="1" maxOccurs="1" type="xsd:string"/>
			<xsd:element name="deviceClass" minOccurs="1" maxOccurs="1" type="uo:DeviceClass"/>
			<xsd:element name="wattage" minOccurs="0" maxOccurs="1" type="xsd:unsignedInt"/>
			<xsd:element name="dcPeriod" minOccurs="0" maxOccurs="1" type="xsd:unsignedInt">
				<xsd:annotation>
					<xsd:documentation>
						The total amount of time, in minutes, which a duty cycle event uses to calculate
						the amount of time the device should be on/off. 
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>	
	
	<!-- Variables -->

	<xsd:complexType name="GetVariablesParam">
		<xsd:annotation>
			<xsd:documentation>
				Parameter for retrieving all variables of a certain type
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="type" minOccurs="1" maxOccurs="1" type="uo:UDIVariableType"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="GetVariableParam">
		<xsd:annotation>
			<xsd:documentation>
				Parameter for retrieving the value of a specific variable
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="type" minOccurs="1" maxOccurs="1" type="uo:UDIVariableType"/>
			<xsd:element name="id" minOccurs="1" maxOccurs="1" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
		
	<xsd:complexType name="SetVariableParam">
		<xsd:annotation>
			<xsd:documentation>
				Parameter for setting a variable to the desired value
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="type" minOccurs="1" maxOccurs="1" type="uo:UDIVariableType"/>
			<xsd:element name="id" minOccurs="1" maxOccurs="1" type="xsd:string"/>
			<xsd:element name="val" minOccurs="1" maxOccurs="1" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>	
	
		
	
	<!-- MESSAGE TYPES -->
	<xsd:complexType name="UDIDefaultResponse">
		<xsd:sequence>
			<xsd:element name="status" minOccurs="1" maxOccurs="1" type="xsd:string"/>
			<xsd:element name="info" minOccurs="0" maxOccurs="1" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="UDITimeResponse">
		<xsd:attribute name="val" type="xsd:dateTime" use="required">
			<xsd:annotation>
				<xsd:documentation>
					Timestamp in the form of YYYYMMDD HH:MM:SS
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>
	
	<xsd:complexType name="UDIIntResponse">
		<xsd:attribute name="val" type="xsd:int" use="required">
			<xsd:annotation>
				<xsd:documentation>
					Return value of type integer. Currently used for IsSubscribed
				</xsd:documentation>
			</xsd:annotation>
		</xsd:attribute>
	</xsd:complexType>


	
	<xsd:element name="UDIService" type="u:UDIServiceParam"/>
	<xsd:element name="Empty" type="uo:Empty"/>
	<xsd:element name="GetNodesConfig" type="uo:Empty"/>
	<xsd:element name="GetISYConfig" type="uo:Empty"/>
	<xsd:element name="AddGroup" type="u:IDNameNodeTypeParam"/>
	<xsd:element name="RemoveGroup" type="u:IDParam"/>
	<xsd:element name="RenameGroup" type="u:IDNameParam"/>
	<xsd:element name="AddFolder" type="u:IDNameParam"/>
	<xsd:element name="RemoveFolder" type="u:IDParam"/>
	<xsd:element name="RenameFolder" type="u:IDNameParam"/>
	<xsd:element name="AddNode" type="u:AddNodeParam"/>
	<xsd:element name="RemoveNode" type="u:IDParam"/>
	<xsd:element name="RenameNode" type="u:IDNameParam"/>
	<xsd:element name="MoveNode" type="u:MoveNodeParam"/>
	<xsd:element name="RemoveFromGroup" type="u:NodeGroupParam"/>
	<xsd:element name="RenameNetwork" type="u:NameParam"/>
	<xsd:element name="QueryAll" type="u:QueryParam"/>
	<xsd:element name="SetDeviceLinkMode" type="u:LinkingModeParam"/>
	<xsd:element name="DiscoverNodes" type="u:DiscoverNodesParam"/>
	<xsd:element name="CancelNodesDiscovery" type="u:StopNodesDiscoveryParam"/>
	<xsd:element name="InternetAccess" type="u:InternetAccessParam"/>
	<xsd:element name="GetSystemStatus" type="uo:Empty"/>
	<xsd:element name="GetSystemTime" type="uo:Empty"/>
	<xsd:element name="SetSystemTime" type="uo:DateTime"/>
	<xsd:element name="GetSystemOptions" type="uo:Empty"/>
	<xsd:element name="SetNotOptions" type="uo:NotificationsOptions"/>
	<xsd:element name="SetProgramOptions" type="uo:ProgramOptions"/>
	<xsd:element name="SetNTPOptions" type="uo:NTPOptions"/>
	<xsd:element name="SynchWithNTS" type="uo:Empty"/>
	<xsd:element name="GetSMTPConfig" type="uo:Empty"/>
	<xsd:element name="SetSMTPConfig" type="uo:SMTPConfiguration"/>
	<xsd:element name="Reboot" type="uo:Empty"/>
	<xsd:element name="SetUserCredentials" type="u:CredentialsParam"/>
	<xsd:element name="RestoreDevicesFromNodes" type="u:RestoreDevicesParam"/>
	<xsd:element name="RestoreDeviceFromNode" type="u:RestoreDeviceParam"/>
	<xsd:element name="ReplaceModem" type="uo:Empty"/>
	<xsd:element name="RemoveModem" type="uo:Empty"/>
	<xsd:element name="ReplaceDevice" type="u:ReplaceDeviceParam"/>
	<xsd:element name="SecuritySystemRequest" type="u:SecuritySystemActionParam"/>
	<xsd:element name="GetDebugLevel" type="uo:Empty"/>
	<xsd:element name="SetDebugLevel" type="u:SetDebugLevelParam"/>
	<xsd:element name="GetSceneProfiles" type="u:NodeControllerParam"/>
	<xsd:element name="SetSceneProfile" type="u:SetSceneProfileParam"/>
	<xsd:element name="Subscribe" type="u:SubscriptionParam"/>
	<xsd:element name="Unsubscribe" type="u:UnsubscriptionParam"/>
	<xsd:element name="IsSubscribed" type="u:IsSubscribedParam"/>
	<xsd:element name="GetCurrentSystemStatus" type="u:CurrentSystemStatusParam"/>
	<xsd:element name="SendTestEmail" type="u:IDParam"/>
	<xsd:element name="ClearLastError" type="uo:Empty"/>
	<xsd:element name="GetLastError" type="uo:Empty"/>
	<xsd:element name="SetNodeEnabled" type="u:SetNodeEnabledParam"/>
	<xsd:element name="SetParent" type="u:NodeHierarchyParam"/>
	<xsd:element name="IsDDNSHostAvail" type="u:DDNSHostParam"/>
	<xsd:element name="GetDDNSHost" type="uo:Empty"/>
	<xsd:element name="AddDDNSHost" type="u:DDNSHostAndIPParam"/>
	<xsd:element name="RemoveDDNSHost" type="uo:DDNSHost"/>
	<xsd:element name="WriteDeviceUpdates" type="u:NodeParam"/>
	<xsd:element name="SetBatchMode" type="u:BatchModeParam"/>
	<xsd:element name="SetBatteryDeviceWriteMode" type="u:BatteryDeviceWriteModeParam"/>
	<xsd:element name="SetNodePowerInfo" type="u:PowerInfoParam"/>
	<xsd:element name="GetStartupTime" type="uo:Empty"/>
	<xsd:element name="SendHeartbeat" type="uo:Empty"/>
	<!-- variables -->
	<xsd:element name="SetVariable" type="u:SetVariableParam"/>
	<xsd:element name="GetVariable" type="u:GetVariableParam"/>
	<xsd:element name="GetVariables" type="u:GetVariablesParam"/>
	
	<!--  Event -->
	<xsd:element name="Event" type="ue:Event"/>
	
	</xsd:schema>
	</wsdl:types>


	<!-- MESSAGES -->
	<wsdl:message name="DefaultResponseMessage">
		<wsdl:part name="response" type="u:UDIDefaultResponse"/>
	</wsdl:message>
	<wsdl:message name="TimeResponseMessage">
		<wsdl:part name="timeResponse" type="u:UDITimeResponse"/>
	</wsdl:message>
	<wsdl:message name="IntResponseMessage">
		<wsdl:part name="intResponse" type="u:UDIIntResponse"/>
	</wsdl:message>
	

	<wsdl:message name="UDIServiceRequest">
		<wsdl:part name="UDIService" element="u:UDIService"/>
	</wsdl:message>

	<wsdl:message name="GetNodesConfigRequest">
		<wsdl:part name="GetNodesConfig" element="u:GetNodesConfig"/>
	</wsdl:message>

	<wsdl:message name="GetNodesConfigResponse">
		<wsdl:part name="nodes" type="uo:nodes"/>
	</wsdl:message>

	<wsdl:message name="GetISYConfigRequest">
		<wsdl:part name="GetISYConfig" element="u:GetISYConfig"/>
	</wsdl:message>

	<wsdl:message name="GetISYConfigResponse">
		<wsdl:part name="configuration" type="uo:configuration"/>
	</wsdl:message>

	<wsdl:message name="AddGroupRequest">
		<wsdl:part name="AddGroup" element="u:AddGroup"/>
	</wsdl:message>

	<wsdl:message name="RemoveGroupRequest">
		<wsdl:part name="RemoveGroup" element="u:RemoveGroup"/>
	</wsdl:message>

	<wsdl:message name="RenameGroupRequest">
		<wsdl:part name="RenameGroup" element="u:RenameGroup"/>
	</wsdl:message>
	
	<wsdl:message name="AddFolderRequest">
		<wsdl:part name="AddFolder" element="u:AddFolder"/>
	</wsdl:message>	
	
	<wsdl:message name="RemoveFolderRequest">
		<wsdl:part name="RemoveFolder" element="u:RemoveFolder"/>
	</wsdl:message>	
	
	<wsdl:message name="RenameFolderRequest">
		<wsdl:part name="RenameFolder" element="u:RenameFolder"/>
	</wsdl:message>	

	<wsdl:message name="AddNodeRequest">
		<wsdl:part name="AddNode" element="u:AddNode"/>
	</wsdl:message>

	<wsdl:message name="RemoveNodeRequest">
		<wsdl:part name="RemoveNode" element="u:RemoveNode"/>
	</wsdl:message>

	<wsdl:message name="RenameNodeRequest">
		<wsdl:part name="RenameNode" element="u:RenameNode"/>
	</wsdl:message>

	<wsdl:message name="MoveNodeRequest">
		<wsdl:part name="MoveNode" element="u:MoveNode"/>
	</wsdl:message>

	<wsdl:message name="RemoveFromGroupRequest">
		<wsdl:part name="RemoveFromGroup" element="u:RemoveFromGroup"/>
	</wsdl:message>

	<wsdl:message name="RenameNetworkRequest">
		<wsdl:part name="RenameNetwork" element="u:RenameNetwork"/>
	</wsdl:message>

	<wsdl:message name="QueryRequest">
		<wsdl:part name="QueryAll" element="u:QueryAll"/>
	</wsdl:message>

	<wsdl:message name="SetLinkingModeRequest">
		<wsdl:part name="SetDeviceLinkMode" element="u:SetDeviceLinkMode"/>
	</wsdl:message>

	<wsdl:message name="StartNodesDiscoveryRequest">
		<wsdl:part name="DiscoverNodes" element="u:DiscoverNodes"/>
	</wsdl:message>

	<wsdl:message name="StopNodesDiscoveryRequest">
		<wsdl:part name="CancelNodesDiscovery" element="u:CancelNodesDiscovery"/>
	</wsdl:message>

	<wsdl:message name="InternetAccessRequest">
		<wsdl:part name="InternetAccess" element="u:InternetAccess"/>
	</wsdl:message>

	<wsdl:message name="GetSystemStatusRequest">
		<wsdl:part name="GetSystemStatus" element="u:GetSystemStatus"/>
	</wsdl:message>

	<wsdl:message name="GetSystemStatusResponse">
		<wsdl:part name="SysStat" type="uo:SystemStatus"/>
	</wsdl:message>

	<wsdl:message name="GetDateTimeRequest">
		<wsdl:part name="GetSystemTime" element="u:GetSystemTime"/>
	</wsdl:message>

	<wsdl:message name="GetDateTimeResponse">
		<wsdl:part name="DT" type="uo:DateTime"/>
	</wsdl:message>

	<wsdl:message name="SetDateTimeRequest">
		<wsdl:part name="SetSystemTime" element="u:SetSystemTime"/>
	</wsdl:message>

