{
  "name": "Rectangular (Small)",
  "base-type": "button",
  "shape": "rectangle",
  "style": "gloss2",
  "constraints": "$0.width = $0.height * 2 :: $0.height = 50",
  "titles": {
    "normal": {
      "font": "Helvetica Neue@20",
      "foreground-color": "white",
      "stroke-color": "white@50%",
      "stroke-width": -2,
      "alignment": "center",
      "text": "Rectangular Button"
    },
    "highlighted.foreground-color": "#00AFFFFF"
  },
  "background-colors.normal": "#666666FF",
  "title-edge-insets": "{20, 20, 20, 20}"
},
