{
  "name": "Transport",
  "baseType": "button-group",
  "role": "transport",
  "constraints": [
    "$0.height = $0.width",
    "$1.left = $9.right :: $1.top = $0.top :: $1.width = $9.width :: $9.left = $0.left",
    "$2.height = $1.height :: $2.left = $1.left :: $2.right = $1.right :: $2.top = $1.bottom :: $1.bottom = $9.bottom",
    "$3.left = $0.left :: $3.right = $9.right :: $3.top = $9.bottom",
    "$4.bottom = $2.bottom :: $4.left = $5.left :: $4.right = $0.right :: $4.top = $5.bottom",
    "$5.bottom = $1.bottom :: $5.left = $1.right :: $5.right = $0.right :: $5.top = $0.top :: $5.width = $9.width",
    "$6.left = $0.left :: $6.right = $3.right :: $6.top = $3.bottom :: $3.bottom = $2.bottom",
    "$7.bottom = $0.bottom :: $7.height = $1.height :: $7.left = $2.left :: $7.right = $2.right :: $7.top = $2.bottom",
    "$8.bottom = $0.bottom :: $8.left = $4.left :: $8.right = $0.right :: $8.top = $4.bottom",
    "$9.top = $0.top :: $6.bottom = $0.bottom"
  ],
  "backgroundColor": "white@75%",
  "subelements": [
    {
      "baseType": "button",
      "role": "play",
      "shape": "rounded-rectangle",
      "style": "gloss2",
      "titles": {
        "normal": <@include TitleAttributes_Default_Icon.json,ICON=play,SIZE=32>,
        "disabled.foregroundColor": <@include ForegroundColor_Disabled.json>,
        "highlighted.foregroundColor": <@include ForegroundColor_Highlighted.json>
      },
      "backgroundColors.normal": <@include BackgroundColor_Dark_Normal.json>,
      "titleEdgeInsets": <@include TitleEdgeInsets_Default.json>
    },
    {
      "baseType": "button",
      "role": "pause",
      "shape": "rounded-rectangle",
      "style": "gloss2",
      "titles": {
        "normal": <@include TitleAttributes_Default_Icon.json,ICON=pause,SIZE=32>,
        "disabled.foregroundColor": <@include ForegroundColor_Disabled.json>,
        "highlighted.foregroundColor": <@include ForegroundColor_Highlighted.json>
      },
      "backgroundColors.normal": <@include BackgroundColor_Dark_Normal.json>,
      "titleEdgeInsets": <@include TitleEdgeInsets_Default.json>
    },
    {
      "baseType": "button",
      "role": "rewind",
      "shape": "rounded-rectangle",
      "style": "gloss2",
      "titles": {
        "normal": <@include TitleAttributes_Default_Icon.json,ICON=backward,SIZE=32>,
        "disabled.foregroundColor": <@include ForegroundColor_Disabled.json>,
        "highlighted.foregroundColor": <@include ForegroundColor_Highlighted.json>
      },
      "backgroundColors.normal": <@include BackgroundColor_Dark_Normal.json>,
      "titleEdgeInsets": <@include TitleEdgeInsets_Default.json>
    },
    {
      "baseType": "button",
      "role": "fast-forward",
      "shape": "rounded-rectangle",
      "style": "gloss2",
      "titles": {
        "normal": <@include TitleAttributes_Default_Icon.json,ICON=fast-forward,SIZE=32>,
        "disabled.foregroundColor": <@include ForegroundColor_Disabled.json>,
        "highlighted.foregroundColor": <@include ForegroundColor_Highlighted.json>
      },
      "backgroundColors.normal": <@include BackgroundColor_Dark_Normal.json>,
      "titleEdgeInsets": <@include TitleEdgeInsets_Default.json>
    },
    {
      "baseType": "button",
      "role": "stop",
      "shape": "rounded-rectangle",
      "style": "gloss2",
      "titles": {
        "normal": <@include TitleAttributes_Default_Icon.json,ICON=stop,SIZE=32>,
        "disabled.foregroundColor": <@include ForegroundColor_Disabled.json>,
        "highlighted.foregroundColor": <@include ForegroundColor_Highlighted.json>
      },
      "backgroundColors.normal": <@include BackgroundColor_Dark_Normal.json>,
      "titleEdgeInsets": <@include TitleEdgeInsets_Default.json>
    },
    {
      "baseType": "button",
      "role": "replay",
      "shape": "rounded-rectangle",
      "style": "gloss2",
      "titles": {
        "normal": <@include TitleAttributes_Default_Icon.json,ICON=step-backward,SIZE=32>,
        "disabled.foregroundColor": <@include ForegroundColor_Disabled.json>,
        "highlighted.foregroundColor": <@include ForegroundColor_Highlighted.json>
      },
      "backgroundColors.normal": <@include BackgroundColor_Dark_Normal.json>,
      "titleEdgeInsets": <@include TitleEdgeInsets_Default.json>
    },
    {
      "baseType": "button",
      "role": "tuck",
      "titles.normal": {
        "font": "FontAwesome@24",
        "foregroundColor": "gray@75%",
        "textEffect": "letter-press",
        "iconName": "chevron-down"
      },
      "titleEdgeInsets": <@include TitleEdgeInsets_Default.json>
    },
    {
      "baseType": "button",
      "role": "skip",
      "shape": "rounded-rectangle",
      "style": "gloss2",
      "titles": {
        "normal": <@include TitleAttributes_Default_Icon.json,ICON=step-forward,SIZE=32>,
        "disabled.foregroundColor": <@include ForegroundColor_Disabled.json>,
        "highlighted.foregroundColor": <@include ForegroundColor_Highlighted.json>
      },
      "backgroundColors.normal": <@include BackgroundColor_Dark_Normal.json>,
      "titleEdgeInsets": <@include TitleEdgeInsets_Default.json>
    },
    {
      "baseType": "button",
      "role": "record",
      "shape": "rounded-rectangle",
      "style": "gloss2",
      "titles": {
        "normal": <@include TitleAttributes_Default_Icon.json,ICON=circle,SIZE=32>,
        "disabled.foregroundColor": <@include ForegroundColor_Disabled.json>,
        "highlighted.foregroundColor": <@include ForegroundColor_Highlighted.json>
      },
      "backgroundColors.normal": <@include BackgroundColor_Dark_Normal.json>,
      "titleEdgeInsets": <@include TitleEdgeInsets_Default.json>
    }
  ]
}
